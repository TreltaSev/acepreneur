{
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "autopep8.args": [
    "--max-line-length",
    "120"
  ],
  "cSpell.words": [
    "acepreneur",
    "authform",
    "buildx",
    "dotenv",
    "esports",
    "isjson",
    "Preneur"
  ],
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  }
}
